(run){
  EVENTS = 1000000
  # 3hr to compute xsec
  # 1:45hr to do 100k w/ a bit different enhance_factors
}(run)

(beam){
  BEAM_1 = 2212; BEAM_ENERGY_1 = 7000;
  BEAM_2 = 2212; BEAM_ENERGY_2 = 7000;
}(beam)

(processes){
  Process 93 93 -> 13 -13 93{3}
  Order_EW 2;
  CKKW sqr(30/E_CMS)
  Enhance_Factor 10 {1}
  Enhance_Factor 100 {2}
  Enhance_Factor 1000 {3}
  End process;
}(processes)

(me){
  EVENT_GENERATION_MODE Weighted;
}(me)

(selector){
  Mass 13 -13 100 200
}(selector)
