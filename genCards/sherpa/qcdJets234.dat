(run){
  EVENTS = 100000
}(run)

(beam){
  BEAM_1 2212; BEAM_ENERGY_1 7000;
  BEAM_2 2212; BEAM_ENERGY_2 7000;
}(beam)

(processes){
  Process 93 93 -> 93 93 93{2};
  Enhance_Observable VAR{max(PPerp(p[2]),PPerp(p[3]))}|100|2000
  Order_EW 0; 
  CKKW sqr(30/E_CMS);
  End process;
}(processes)

(selector){
  Mass 13 -13 20. E_CMS
  NJetFinder 2 20. 0 0.4;
}(selector)

(me){
  EVENT_GENERATION_MODE = Weighted
}(me)
